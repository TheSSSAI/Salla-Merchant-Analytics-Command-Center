{
  "name": "salla-analytics-data-pipeline",
  "version": "1.0.0",
  "private": true,
  "description": "Background service for data synchronization and ETL processes",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "test": "jest"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.0",
    "@prisma/client": "^5.10.0",
    "@upstash/qstash": "^2.5.0",
    "@upstash/redis": "^1.28.0",
    "axios": "^1.6.0",
    "axios-retry": "^4.0.0",
    "date-fns": "^3.3.0",
    "dotenv": "^16.4.0",
    "fastify": "^4.26.0",
    "ioredis": "^5.3.0",
    "p-limit": "^5.0.0",
    "p-retry": "^6.2.0",
    "postmark": "^4.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "prisma": "^5.10.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}